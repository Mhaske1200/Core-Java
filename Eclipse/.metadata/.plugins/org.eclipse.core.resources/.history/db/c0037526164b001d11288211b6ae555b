package Instruction;

public class Thread_Divisor implements Runnable 
{
	Thread t;
	int num;
	
	public Thread_Divisor() 
	{
		t = new Thread(this);
		num=0;
	}
	public Thread_Divisor(int num) 
	{
		t = new Thread(this);
		this.num= num;
	}
	
	public Thread getT() 
	{
		return t;
	}
	@Override
	public void run() 
	{
		

	}

}
